<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.imac.nutc.chart.main.MainActivity">

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.7"
        android:id="@+id/network_editText"
        android:background="@drawable/bg_edittext"
        android:maxLines="1"
        android:inputType="text"
        android:hint=" network id"/>

    <Button
        android:text="確認"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/network_button"
        android:layout_marginTop="144dp"
        app:layout_constraintTop_toBottomOf="@+id/network_editText"
        app:layout_constraintRight_toRightOf="@+id/network_editText"
        app:layout_constraintLeft_toLeftOf="@+id/network_editText"
        app:layout_constraintHorizontal_bias="0.0" />

</android.support.constraint.ConstraintLayout>
